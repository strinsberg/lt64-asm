(lt64-asm-prog
  (static
    (:word A 10 1 8 3 2 7 0 6 3 4 9)
    (:word i 1 1))

  (main
    ;; Load A[0]
    :push A
    :load-lb

    ;; Loop over nums
    :label loop

    ;; Load num at A[i]
    :push i
    :load-lb
    :push A
    :add
    :load-lb

    ;; call max function
    :push max
    :call

    ;; Check if we have run them all
    :push i
    :push 10
    :eq
    :push end-loop
    :branch
    
    ;; not finished increment i and loop
    :push i
    :push inc-at
    :call
    :push loop
    :jump

    ;; finished
    :label end-loop
    :wprn
    :push 10
    :prnch
    :halt)

  ;; find max of two numbers on the stack
  (proc max
    :gt
    :push second-larger
    :branch
    :swap
    :label second-larger
    :pop
    :ret)

  (proc inc-at
    :load-lb
    :first
    :push 1
    :add
    :store-lb
    :ret)

  ;; include something for testing, but not important for the program
  ;;(include "test/lt64_asm/test_mod1.lta")
)
